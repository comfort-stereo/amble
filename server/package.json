{
  "name": "@amble/server",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "dev": "ts-node-dev --ignore-watch '../client/.next' index.ts ",
    "codegen": "ts-node -T codegen.ts && prettier --write ../common/generated",
    "typeorm": "ts-node ./node_modules/typeorm/cli -f ./src/ormconfig.ts"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./orm.config.ts"
    ]
  },
  "dependencies": {
    "apollo-server": "^2.18.2",
    "apollo-server-cache-redis": "^1.2.2",
    "bcrypt": "^5.0.0",
    "class-validator": "^0.12.2",
    "cookie-parser": "^1.4.5",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "graphql-redis-subscriptions": "^2.3.1",
    "graphql-subscriptions": "^1.1.0",
    "ioredis": "^4.17.3",
    "is-docker": "^2.1.1",
    "jsonwebtoken": "^8.5.1",
    "mikro-orm": "^3.6.15",
    "pg": "^8.4.1",
    "promise-fs": "^2.1.1",
    "reflect-metadata": "^0.1.13",
    "supertest": "^5.0.0",
    "type-graphql": "^1.1.0",
    "typedi": "^0.8.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/ioredis": "^4.17.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.23",
    "@types/object-hash": "^1.3.4",
    "@types/pg": "^7.14.4",
    "@types/promise-fs": "^2.1.0",
    "@types/react": "^16.9.43",
    "@types/supertest": "^2.0.10",
    "object-hash": "^2.0.3"
  }
}
